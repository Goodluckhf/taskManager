{
	"sourceMaps": true,
	"ignore": [
		"node_modules",
		"test.js",
		"spec.js",
		"coverage",
		"dist",
		".nyc_output",
		"services/web"
	],
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"node": "current"
				},
				"useBuiltIns": "usage",
				"shippedProposals": true,
				"loose": false
			}
		],
		"@babel/typescript"
	],
	"plugins": [
		"babel-plugin-transform-typescript-metadata",
		["@babel/plugin-proposal-decorators", { "legacy": true }],
		["@babel/proposal-class-properties", { "loose": true }],
		"@babel/proposal-object-rest-spread"
	]
}
