{
  "name": "www",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon --require babel-register app.js",
    "precommit": "lint-staged"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.5.2",
    "amqplib": "^0.5.2",
    "axios": "^0.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bluebird": "^3.5.1",
    "bunyan": "^1.8.12",
    "bunyan-stdout-stream": "^1.3.0",
    "config": "^1.31.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-morgan": "^1.0.1",
    "koa-router": "^7.4.0",
    "moment": "^2.22.2",
    "mongoose": "^5.2.7",
    "mongoose-paginate": "^5.0.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.13.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --quiet"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint-config-airbnb-base"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-multi-spaces": 0,
      "no-underscore-dangle": "warn",
      "indent": [
        "error",
        "tab"
      ],
      "no-tabs": 0,
      "key-spacing": [
        "error",
        {
          "align": {
            "beforeColon": false,
            "afterColon": true,
            "on": "colon"
          }
        }
      ],
      "spaced-comment": 0,
      "no-trailing-spaces": 0,
      "arrow-body-style": [
        1,
        "as-needed"
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage",
          "loose": true
        }
      ]
    ],
    "plugins": [
      [
        "syntax-object-rest-spread"
      ]
    ]
  }
}
