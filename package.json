{
  "name": "www",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start:dev": "node --require @babel/register app.js",
    "test": "npm run test:api",
    "test:api": "mocha --exit --ui bdd --reporter spec --require @babel/register ./services/api/tests/setup.js ./services/api/tests/**/*.test.js ./services/api/tests/*.test.js ./services/api/*.test.js",
    "test:coverage:dev": "nyc --reporter=text mocha --exit --ui bdd --reporter spec --require @babel/register ./services/api/tests/setup.js ./services/api/tests/**/*.test.js ./services/api/tests/*.test.js",
    "precommit": "lint-staged",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.42",
    "ajv": "6.5.3",
    "amqplib": "0.5.2",
    "axios": "^0.19.0",
    "bluebird": "3.5.1",
    "bunyan": "1.8.12",
    "bunyan-stdout-stream": "^1.5.2",
    "cheerio": "0.22.0",
    "config": "1.31.0",
    "dotenv": "6.0.0",
    "jsonwebtoken": "8.3.0",
    "koa": "2.5.2",
    "koa-bodyparser": "4.2.1",
    "koa-morgan": "1.0.1",
    "koa-passport": "4.1.1",
    "koa-router": "7.4.0",
    "lodash": "^4.17.15",
    "moment": "2.22.2",
    "mongoose": "^5.8.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "source-map-support": "^0.5.9",
    "umetrics": "^1.0.4",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "mocha": "^5.2.0",
    "mockgoose": "^8.0.4",
    "nyc": "^14.1.1",
    "prettier": "^1.15.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix --ignore-path .gitignore",
      "prettier --write",
      "git add"
    ]
  },
  "nyc": {
    "include": [
      "services/api/**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "services/api/tests/setup.js",
      "services/api/routes",
      "services/api/app.js",
      "services/api/passport.js"
    ],
    "all": true
  }
}
